{
  "name": "quick-readme",
  "displayName": "QuickReadme",
  "description": "VS Code extension to auto-generate README.md with proyect  structure and techs  ",
  "version": "0.0.11",
  "publisher": "jochylazala",
  "icon": "./images/icon.png",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "readme-generator.createReadme",
        "title": "Generate README.md"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lazalajochy/readme.md-generator"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "xml2js": "^0.6.2"
  }
}
